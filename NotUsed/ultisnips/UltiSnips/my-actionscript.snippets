snippet function
    ${1:public} function ${2}(${3}):${3:void} {
    ${4}
    }
endsnippet

snippet Embed
    [Embed(source='${1}')]
endsnippet

snippet var
    ${1:public} var ${2}:${3};
endsnippet

snippet if
    if(${1})
    {
        ${2}
    }
endsnippet

snippet for
    for( ${1} ${2:i} = ${3}; $2 < ${4:count}; $2 += ${5})\n{\n${6}\n}\n
endsnippet

